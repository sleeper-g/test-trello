{
  "name": "dnd-trello",
  "version": "1.0.0",
  "main": "index.js",
  "author": "sleeper-g",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint . --fix",
    "test": "jest --testPathPattern=./src/ --passWithNoTests --detectOpenHandles",
    "testcover": "jestjest --testPathPattern=./src/ --passWithNoTests --coverage",
    "doc": "jsdoc -c jsdoc.conf.json",
    "preshow:coverage": "npm run testcover",
    "show:coverage": "live-server testcover/lcov-report",
    "preshow:dist": "npm run build",
    "show:dist": "live-server dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.1",
    "prettier": "^3.3.3",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1"
  }
}
